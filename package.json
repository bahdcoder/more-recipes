{
  "name": "more-recipes",
  "version": "1.0.0",
  "description": "Bahdcoder Andela Cycle 28 Boot-camp Project",
  "main": "server/index.js",
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development ./node_modules/.bin/babel-watch server/index.js",
    "start": "cross-env NODE_ENV=production node build/index.js",
    "start:e2e": "cross-env NODE_ENV=e2eTests node build/index.js",
    "babel-node": "babel-node --",
    "nightwatch": "nightwatch",
    "build": "./node_modules/.bin/babel server/ -d build --copy-files",
    "dist": "./node_modules/.bin/babel server/ -d dist --presets=babili",
    "coveralls": "nyc npm test&& nyc report --reporter=text-lcov | coveralls",
    "migrate:refresh": "./node_modules/.bin/sequelize db:migrate:undo:all && ./node_modules/.bin/sequelize db:migrate",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/mocha server/test/**/**.spec.js --exit --timeout 10000  --compilers js:babel-core/register --reporter spec",
    "test:client": "cross-env NODE_ENV=test ./node_modules/.bin/mocha client/src/js/test/**/**/**.spec.js --exit --timeout 10000  --compilers js:babel-core/register --reporter spec",
    "build:client": "node server/scripts/removeDeploymentDir.js && cross-env NODE_ENV=production webpack -p",
    "dev:client": "webpack-dev-server --hot --inline --history-api-fallback",
    "heroku-postbuild": "npm run build:client && npm run build",
    "start:worker:mails": "cross-env NODE_ENV=production node server/workers/mails.js",
    "start:worker:mails:dev": "cross-env NODE_ENV=development node server/workers/mails.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kati-frantz/more-recipes.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kati-frantz/more-recipes/issues"
  },
  "homepage": "https://github.com/kati-frantz/more-recipes#readme",
